---
title: "{{ page.annotations.get('title') }}"
date: "{{ page.last_message_time }}"
tags:
  - autogenerated
  - source__{{page.annotations['source']}}
  - conversation_id__{{page.conversation_id}}
{%- for msg_id in page.get_message_ids() %}
  - msg_{{ msg_id }}
{%- endfor %}
---
{% for message in page.messages %}
**{{ message.author_role.upper() }}**

{{ message.content.strip() }}

{% endfor -%}
{%- if page.annotations.get('previous_title') or page.annotations.get('next_title') %}
---
{%- if page.annotations.get('previous_title') %}
preceding: [[{{ page.annotations.get('previous_title') }}]]  
{%- endif %}
{%- if page.annotations.get('next_title') %}
following: [[{{ page.annotations.get('next_title') }}]]
{%- endif %}
{% endif %}